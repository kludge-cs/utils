{
	"name": "@kludge-cs/js-eslint-config",
	"version": "1.0.0",
	"description": "ESLint config for KCS JavaScript projects.",
	"author": {
		"name": "Nytelife26 (Tyler J. Russell)",
		"email": "nytelife@protonmail.com",
		"url": "https://github.com/Nytelife26"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js"
	},
	"homepage": "https://github.com/kludge-cs/utils/tree/main/packages/js-eslint-config",
	"scripts": {
		"test": "jest",
		"prelint": "npm run test && npm run build",
		"lint": "eslint src tests --ext ts --fix -c dist/index.js",
		"build": "tsc -b src",
		"prepare": "npm run build"
	},
	"peerDependencies": {
		"eslint": "7.x",
		"eslint-plugin-json": "2.x"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kludge-cs/utils.git",
		"directory": "packages/js-eslint-config"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"keywords": [
		"eslint"
	],
	"bugs": {
		"url": "https://github.com/kludge-cs/utils/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"eslint-plugin-json": "^2.1.2"
	}
}
